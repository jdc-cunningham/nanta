{
  "name": "NANTA",
  "version": "1.0",
  "description": "Save notes and tabs, view previous entries",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "http://192.168.1.144:*/*"
  ],
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
      "16": "icon16_dark.png",
      "48": "icon48_dark.png",
      "128": "icon128_dark.png"
  },
  "action": {
    "default_popup": "./ui/index.html"
  }
}